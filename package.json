{
  "name": "is-js-obj-empty",
  "version": "1.0.0",
  "description": "checks the given js object is empty",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover tape test.js",
    "jshint": "jshint -c .jshintrc *.js",
    "code-style": "jscs -p google index.js *.js",
    "check-coverage": "istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "open-coverage": "open coverage/lcov-report/index.html"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joaquimserafim/is-js-obj-empty.git"
  },
  "keywords": [
    "object",
    "js",
    "empty"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/is-js-obj-empty/issues"
  },
  "homepage": "https://github.com/joaquimserafim/is-js-obj-empty",
  "devDependencies": {
    "istanbul": "^0.3.5",
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "pre-commit": "0.0.11",
    "tape": "^3.4.0",
    "which": "^1.0.8"
  },
  "pre-commit": [
    "jshint",
    "code-style",
    "test",
    "check-coverage"
  ]
}
